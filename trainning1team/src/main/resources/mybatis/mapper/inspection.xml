<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bnksystem.trainning1team.mapper.InspectionMapper">
    <delete id="deleteInspection">
        DELETE FROM tb_inspection
    </delete>

    <insert id="insertInspection">
        INSERT INTO tb_inspection (eq_id)
        SELECT id
        FROM tb_equipments
    </insert>
</mapper>